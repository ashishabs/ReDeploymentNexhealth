<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>At_Risk_Numeric__c</fullName>
    <description>Indicates if an Account&#39;s Risk Level is filled in and not Low. Used for Account Hierarchy package rollups.</description>
    <externalId>false</externalId>
    <formula>IF(OR( ISPICKVAL(Account_Risk_Level__c, &quot;&quot;),
      ISPICKVAL(Account_Risk_Level__c, &quot;Low&quot;)

      ), 0, 1
   )</formula>
    <label>At Risk (Numeric)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
